# Production Environment Configuration
ENVIRONMENT=production
AWS_REGION=eu-west-2

# Database (will be set by CloudFormation)
DB_NAME=world_cup_wednesdays
DB_USER=wcw_user

# Application
NODE_ENV=production
PORT=5000

# Features
ENABLE_REGISTRATION=true
ENABLE_AI_ENTRIES=true
LOG_LEVEL=warn

# Monitoring
ENABLE_METRICS=true
HEALTH_CHECK_INTERVAL=30

# Security
HELMET_ENABLED=true
RATE_LIMIT_ENABLED=true