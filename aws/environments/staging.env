# Staging Environment Configuration
ENVIRONMENT=staging
AWS_REGION=eu-west-2

# Database (will be set by CloudFormation)
DB_NAME=world_cup_wednesdays
DB_USER=wcw_user

# Application
NODE_ENV=production
PORT=5000

# Features
ENABLE_REGISTRATION=true
ENABLE_AI_ENTRIES=true
LOG_LEVEL=info

# Monitoring
ENABLE_METRICS=true
HEALTH_CHECK_INTERVAL=30